{
  "name": "debra",
  "version": "1.0.0",
  "description": "Debra",
  "main": "./compiled/main/main.js",
  "scripts": {
    "build:main": "tsc --project config/main/tsconfig.json",
    "build:renderer": "webpack --config config/renderer/webpack.config.js --mode=production",
    "build": "concurrently \"npm:build:main\" \"npm:build:renderer\"",
    "start:main": "tsc --project config/main/tsconfig.json --watch",
    "start:renderer": "webpack --config config/renderer/webpack.config.js --mode=development --watch",
    "start:electron": "electron .",
    "start": "concurrently \"npm:start:main\" \"npm:start:renderer\" \"pm:start:electron\"",
    "dist": "electron-builder"
  },
  "author": "Jan Hellar",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^4.5.0",
    "@monaco-editor/react": "^4.0.11",
    "antd": "^4.13.1",
    "css-loader": "^5.1.2",
    "dayjs": "^1.8.36",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "monaco-editor": "^0.21.3",
    "npm": "^6.14.11",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "tree-kill": "^1.2.2",
    "ts-loader": "^8.0.17",
    "ts-morph": "^10.0.1",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4"
  },
  "devDependencies": {
    "@types/npm": "^2.0.31",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "concurrently": "^6.0.0",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "webpack-cli": "^4.5.0"
  },
  "build": {
    "appId": "org.hellar.debra",
    "extraResources": "bin"
  }
}
