{
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "../../compiled/main/"
  },
  "include": ["../../src/main/**/*"],
  "exclude": ["../../node_modules", "../../**/*.spec.ts"]
}